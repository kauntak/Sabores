  "description": "Sabores App",
  "main": "index.js",
  "dependencies": {
    "react": "^17.0.2",
    "nodemon": "^2.0.19",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.97",
