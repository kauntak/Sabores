{
    "compilerOptions": {
      "target": "esnext",
      "lib":[
        "dom",
        "dom.iterable",
        "esnext"],
      "module": "esnext",
      "outDir": "dist/",
      "rootDir": "server/src",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "strictFunctionTypes": false,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx"
    },
    "include": ["server/src"],
    "exclude": ["sever/src/types/*.ts", "node_modules", "server/node_modules", ".vscode", "type.d.ts"]
  }
  